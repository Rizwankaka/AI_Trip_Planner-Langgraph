# AI Trip Planner Environment Variables
# Copy this file to .env and fill in your actual API keys

# Required: Groq API Key (Get from https://console.groq.com/)
GROQ_API_KEY=your_groq_api_key_here

# Optional: Google Places API Key (Get from https://console.cloud.google.com/)
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Optional: OpenWeather API Key (Get from https://openweathermap.org/api)
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Optional: Currency API Key (Get from your preferred currency API provider)
CURRENCY_API_KEY=your_currency_api_key_here

# Application Settings
DEBUG=False
LOG_LEVEL=INFO
